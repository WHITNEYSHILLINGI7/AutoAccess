{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "AUTOACCESS_SECRET_KEY": "@autoaccess-secret-key",
    "AUTOACCESS_ADMIN_USER": "@autoaccess-admin-user",
    "AUTOACCESS_ADMIN_PASS": "@autoaccess-admin-pass"
  }
}
